    <WebSharperProject>Website</WebSharperProject>
    <WebSharperSourceMap>true</WebSharperSourceMap>
    <WebProjectOutputDir>wwwroot</WebProjectOutputDir>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
